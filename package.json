{
	"name": "@steebchen/id",
	"version": "0.0.0",
	"description": "Simple time-sortable IDs",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"files": [
		"dist"
	],
	"author": "steebchen",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.9.2",
		"semantic-release": "^25.0.1",
		"tsup": "^8.5.0",
		"typescript": "^5.3.0"
	}
}
